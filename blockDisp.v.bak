module blockDisp(in,out);
input [2:0]in;
output [6:0]out;
//..0
//5...1
//..6
//4...2
//..3
//
//
//
//
//
reg [7:0] out;
always@(*)
begin
	case({in})
	3'd0:out=7'd0000000;//empty
	3'd1:out=7'd0100000;//air
	3'd2:out=7'd1001110;//dirt
	3'd3:out=7'd0001000;//ground
	3'd4:out=7'd1100010;//queen
	3'd5:out=7'd1110110;//ant
	3'd6:out=7'd1011100;//sugar
	3'd7:out=7'd0011011;//tunnel
	default:out = 7'b1111111;
	endcase
end
endmodule